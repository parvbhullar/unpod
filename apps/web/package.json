    "desktop:build:all": "bash ../unpod-tauri/scripts/build-all.sh",
    "desktop:icon": "cd ../unpod-tauri && npx tauri icon ../web/public/android-chrome-512x512.png",
    "e2e": "nx e2e web",
    "e2e:ui": "nx e2e web --ui",
    "e2e:headed": "nx e2e web --headed",
    "e2e:debug": "nx e2e web --debug"
