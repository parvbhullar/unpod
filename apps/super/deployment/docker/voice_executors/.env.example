# ===================================
# VOICE EXECUTORS - ENVIRONMENT VARIABLES
# Copy this file to .env and fill in your values
# Shared by: superkik_executor_v1, voice_lk_executor_v3 (QA & Prod)
# ===================================

# Image Configuration
IMAGE_TAG=latest

# SuperKik Configuration (only used by superkik-executor-v1)
SUPERKIK_ENABLE_CALLS=true
SUPERKIK_ENABLE_BOOKING=true
SUPERKIK_ENABLE_PREFERENCES=true
SUPERKIK_MAX_RESULTS=5
SUPERKIK_SEARCH_RADIUS=5.0
SUPERKIK_COMPACT_PROMPT=false

# STT Configuration (Speech-to-Text)
STT_PROVIDER=deepgram
STT_MODEL=nova-3
STT_LANGUAGE=multi

# TTS Configuration (Text-to-Speech)
TTS_PROVIDER=cartesia
TTS_MODEL=sonic-3
TTS_VOICE=95d51f79-c397-46f9-b49a-23763d3eaa2d

# LiveKit Configuration (Required)
LIVEKIT_URL=wss://your-livekit-server.com
LIVEKIT_API_KEY=your_livekit_api_key
LIVEKIT_API_SECRET=your_livekit_api_secret

# AI Services API Keys (Required)
OPENAI_API_KEY=sk-your-openai-key
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key
DEEPGRAM_API_KEY=your_deepgram_api_key
CARTESIA_API_KEY=your_cartesia_api_key

# Google Places (Optional - used by superkik)
GOOGLE_PLACES_API_KEY=

# SIP/Telephony (Optional - used by superkik)
SIP_DOMAIN=
SIP_USERNAME=
SIP_PASSWORD=

# Database Configuration (Required)
MONGO_DSN=mongodb://user:password@host:27017/database
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USER=root
MYSQL_PASSWORD=password
MYSQL_DATABASE=unpod
REDIS_URI=redis://localhost:6379/1
